<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-cascade="none" package="org.sakaiproject.certification.api.criteria.gradebook"
	default-lazy="false">
	<class name="org.sakaiproject.certification.api.criteria.gradebook.CertGradebook" table="GB_GRADEBOOK_T">

		<id name="id" column="ID" type="long">
			<generator class="native">
				<param name="sequence">GB_GRADEBOOK_S</param>
			</generator>
		</id>

		<property name="uid" column="GRADEBOOK_UID" type="string" unique="true" not-null="true"/>

		<property name="category_type" column="CATEGORY_TYPE" type="integer" not-null="true"/>

	</class>
</hibernate-mapping>
